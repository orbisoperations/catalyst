{
  "name": "@catalyst/catalyst-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "vercel-build": "next build",
    "ndev": "next dev --port 4000",
    "pages:dev": "pnpm wrangler pages dev ./.vercel/output/static --port 4000",
    "pages:build": "pnpm next-on-pages",
    "pages:deploy": "pnpm run pages:build && wrangler pages deploy ./.vercel/output",
    "deploymentStaging": "npx @cloudflare/next-on-pages && pnpm wrangler pages deploy --branch staging --env preview",
    "deploymentDemo": "npx @cloudflare/next-on-pages && pnpm wrangler pages deploy --branch demo --env production",
    "bdev": "pnpm pages:build && pnpm pages:dev",
    "dev": "pnpm pages:build --skip-build && pnpm pages:dev",
    "cbuild": "npx @cloudflare/next-on-pages@1",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "@catalyst/issued-jwt-registry": "workspace:*",
    "@catalyst/schema_zod": "workspace:*",
    "@chakra-ui/button": "catalog:",
    "@chakra-ui/card": "catalog:",
    "@chakra-ui/layout": "catalog:",
    "@chakra-ui/react": "catalog:",
    "@chakra-ui/system": "catalog:",
    "@chakra-ui/theme-tools": "catalog:",
    "@cloudflare/next-on-pages": "catalog:",
    "@emotion/react": "catalog:",
    "@emotion/styled": "^11.11.5",
    "@heroicons/react": "catalog:",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "wrangler": "catalog:",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.15",
    "@cloudflare/workers-types": "catalog:",
    "@eslint/js": "catalog:",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "autoprefixer": "catalog:",
    "eslint": "catalog:",
    "eslint-config-next": "14.2.3",
    "postcss": "^8.4.38",
    "prettier": "catalog:",
    "tailwindcss": "^3.4.3",
    "tslog": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
