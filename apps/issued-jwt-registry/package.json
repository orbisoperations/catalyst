{
	"name": "@catalyst/issued-jwt-registry",
	"version": "0.0.1",
	"main": "index.ts",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev --port 4005 --inspector-port 6005",
		"deploy:uxr": "wrangler deploy --env uxr",
		"deploy:staging": "wrangler deploy --env staging",
		"deploy:preview": "wrangler deploy --env preview",
		"test": "vitest",
		"test:unit": "vitest --project unit",
		"test:integration": "vitest --project integration",
		"test:watch": "vitest --project unit --watch",
		"test:workspace": "vitest run --silent",
		"test:coverage": "vitest --coverage",
		"test:coverage:unit": "vitest --project unit --coverage",
		"test:coverage:integration": "vitest --project integration --coverage",
		"lint": "eslint . --ext .ts",
		"build": "wrangler build"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "catalog:",
		"@cloudflare/workers-types": "catalog:",
		"@eslint/js": "catalog:",
		"@typescript-eslint/eslint-plugin": "catalog:",
		"@typescript-eslint/parser": "catalog:",
		"@vitest/coverage-istanbul": "catalog:",
		"eslint": "catalog:",
		"prettier": "catalog:",
		"tsx": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:",
		"wrangler": "catalog:"
	},
	"dependencies": {
		"@catalyst/schemas": "workspace:*",
		"@catalyst/schema_zod": "workspace:*",
		"@catalyst/user-credentials-cache": "workspace:*"
	}
}
