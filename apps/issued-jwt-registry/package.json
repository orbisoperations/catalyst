		"@vitest/coverage-istanbul": "^3.2.4",
		"eslint": "^9.25.1",
		"prettier": "^3.5.3",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4",
