{
  "author": "j.taylor @jtaylor-orbis",
  "bin": {
    "cctl": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@oclif/plugin-update": "^4.1.11",
    "axios": "^1.6.7",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0"
  },
  "description": "Catalyst CLI",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "aws-sdk": "^2.1560.0",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.4.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/OrbisOps/catalyst",
  "license": "Elastic-2.0",
  "main": "",
  "name": "catalyst_cli_2",
  "oclif": {
    "bin": "cctl",
    "dirname": "cctl",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-update"
    ],
    "update": {
      "host": "https://s3.console.aws.amazon.com/",
      "bucket": "catalyst-cli",
      "acl": "bucket-owner-full-control"
    },
    "macos": {
      "identifier": "com.orbisops.catalystcli"
    },
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "OrbisOps/catalyst",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/OrbisOps/catalyst/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
