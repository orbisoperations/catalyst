{
	"name": "ts-app",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@apollo/client": "^3.9.3",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"ink": "^4.4.1",
		"pastel": "^2.0.2",
		"react": "^18.2.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@tsconfig/recommended": "^1.0.3",
		"@types/ink": "^2.0.3",
		"@types/react": "^18.2.55",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.3.1",
		"chalk": "^5.3.0",
		"esbuild": "^0.20.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.8",
		"react-devtools-core": "^5.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
