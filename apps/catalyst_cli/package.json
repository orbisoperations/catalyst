{
  "author": "j.taylor @jtaylor-orbis",
  "bin": {
    "cctl": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "catalog:",
    "@oclif/plugin-help": "catalog:",
    "@oclif/plugin-plugins": "catalog:",
    "axios": "^1.6.8",
    "graphql-request": "catalog:"
  },
  "description": "Catalyst CLI",
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@oclif/prettier-config": "catalog:",
    "@oclif/test": "catalog:",
    "@types/chai": "catalog:",
    "@types/mocha": "catalog:",
    "@types/node": "^20",
    "chai": "catalog:",
    "eslint": "catalog:",
    "eslint-config-oclif": "catalog:",
    "eslint-config-oclif-typescript": "catalog:",
    "eslint-config-prettier": "catalog:",
    "mocha": "catalog:",
    "oclif": "catalog:",
    "shx": "catalog:",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/OrbisOps/catalyst",
  "license": "Elastic-2.0",
  "main": "",
  "name": "catalyst_cli_2",
  "oclif": {
    "bin": "cctl",
    "dirname": "cctl",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "OrbisOps/catalyst",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/OrbisOps/catalyst/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
