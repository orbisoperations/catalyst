{
  "author": "j.taylor @jtaylor-orbis",
  "bin": {
    "cctl": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^6.2.27",
    "@oclif/plugin-plugins": "^4.3.10",
    "axios": "^1.8.4",
    "graphql-request": "^6.1.0"
  },
  "description": "Catalyst CLI",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4.1.12",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.1",
    "chai": "^5.2.0",
    "eslint": "^9.24.0",
    "eslint-config-oclif": "^6.0.44",
    "eslint-config-prettier": "^10.1.2",
    "mocha": "^11.1.0",
    "oclif": "^4.17.44",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/OrbisOps/catalyst",
  "license": "Elastic-2.0",
  "main": "",
  "name": "catalyst_cli_2",
  "oclif": {
    "bin": "cctl",
    "dirname": "cctl",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "OrbisOps/catalyst",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/OrbisOps/catalyst/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
